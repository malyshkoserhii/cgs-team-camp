{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~shared/*": [
                "src/shared/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~typings/*": [
                "src/typings/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~app/*": [
                "src/app/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~LoginPage/*": [
                "src/pages/LoginPage/*"
            ]
        }
    }
}