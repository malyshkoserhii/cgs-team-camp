{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~shared/*": [
                "src/shared/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~typings/*": [
                "src/typings/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~app/*": [
                "src/app/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~helpers/*": [
                "src/shared/helpers/*"
            ],
            "~authForm/*": [
                "src/components/authForm/*"
            ],
            "~changePasswordForm/*": [
                "src/components/changePasswordForm/*"
            ],
            "~ActivatePage/*": [
                "src/pages/ActivatePage/*"
            ],
            "~RegisterPage/*": [
                "src/pages/RegisterPage/*"
            ],
            "~ResetPasswordConfirmPage/*": [
                "src/pages/ResetPasswordConfirmPage/*"
            ],
            "~ResetPasswordPage/*": [
                "src/pages/ResetPasswordPage/*"
            ],
            "~modalContent/*": [
                "src/components/modalContent/*"
            ]
        }
    }
}