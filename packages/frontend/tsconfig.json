{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~modules/*": [
                "src/modules/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~shared/*": [
                "src/shared/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~typings/*": [
                "src/typings/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~keys/*": [
                "src/shared/keys/*"
            ],
            "~button/*": [
                "src/shared/components/button/*"
            ],
            "~loader/*": [
                "src/shared/components/loader/*"
            ],
            "~services/*": [
                "src/shared/services/*"
            ],
            "~components/*": [
                "src/shared/components/*"
            ],
            "~todos/*": [
                "src/modules/todos/*"
            ],
            "~dashboard/*": [
                "src/modules/dashboard/*"
            ],
            "~app/*": [
                "src/modules/app/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ]
        },
        "jsx": "react-jsx"
    }
}